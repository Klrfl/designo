{
  "name": "designo-multi-page-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tinacms build --local -c \"react-router build\"",
    "dev": "tinacms dev -c \"react-router dev\"",
    "start": "react-router-serve ./build/server/index.js",
    "typegen": "wrangler types && react-router typegen",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "preview": "pnpm build && vite preview",
    "deploy": "pnpm build && wrangler deploy"
  },
  "dependencies": {
    "@fontsource-variable/jost": "^5.2.6",
    "@netlify/vite-plugin-react-router": "^1.0.1",
    "@react-router/fs-routes": "^7.7.0",
    "@react-router/node": "^7.7.0",
    "@react-router/serve": "^7.7.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.0",
    "tinacms": "^2.8.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.10.1",
    "@react-router/dev": "^7.7.0",
    "@tailwindcss/vite": "^4.1.4",
    "@tinacms/cli": "^1.10.1",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
